package basicJava;

import java.util.Scanner;

public class Variable {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int a,b,c;
	System.out.println("enter A :");
	a=sc.nextInt();
	System.out.println("enter B :");
	b=sc.nextInt();
	c=a+b;
	System.out.println("addition :"+c);
}
}
