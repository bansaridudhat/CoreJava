package basicJava;
class A1
{
	A1()
	{
		System.out.println("A1 is default Constructor");
	}
	void show()
	{
		System.out.println("show form A1 ");
	}
}
class B1 extends A1
{
	B1(){
		System.out.println("B1 is default Constructor");
	}
	void show()
	{
		System.out.println("Show form B1");
	}
}
class C1 extends B1
{
	C1(){
		System.out.println("c1 is default Constructor");
	}
	void show()
	{
		System.out.println("Show form C1");
	}
}

public class ConstructorChainning {

	public static void main(String[] args) {
		
	
	C1 c=new C1();
c.show();
}
}
